{
  "language": "ja",
  "plugins": [
    "-sharing",
    "hide-published-with",
    "livereload",
    "custom-favicon",
    "-sharing",
    "uml",
    {"name": "mermaid", 
     "version": "git+https://github.com/hypc/gitbook-plugin-mermaid.git"}
  ],
  "dependencies": {
    "plantuml-encoder": "1.2.3"
  },
  "pluginsConfig": {
    "favicon": "./assets/images/icon/favicon.ico",
    "sharing": {
      "facebook": true,
      "twitter": true,
      "google": true,
      "weibo": false,
      "instapaper": false,
      "vk": false,
      "all": [
        "facebook",
        "twitter",
        "google",
        "weibo",
        "instapaper",
        "vk"
      ]
    },
    "uml": {
	    
	    "charset": "utf-8",
	    "format": "svg"
    }
  }
}
